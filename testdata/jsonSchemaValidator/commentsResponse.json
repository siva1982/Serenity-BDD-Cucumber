{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "postId",
    "id",
    "name",
    "email",
    "body"
  ],
  "properties": {
    "postId": {
      "$id": "#/properties/postId",
      "type": "integer",
      "title": "The Postid Schema",
      "default": 0,
      "examples": [
        101
      ]
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        501
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Test Comment"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        "Test@test.biz"
      ],
      "pattern": "^(.*)$"
    },
    "body": {
      "$id": "#/properties/body",
      "type": "string",
      "title": "The Body Schema",
      "default": "",
      "examples": [
        "This is a test body to test post operation"
      ],
      "pattern": "^(.*)$"
    }
  }
}